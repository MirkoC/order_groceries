<div class="col-sm-8 center-block">
  <h4 class="text-center">Admins panel</h4>
  <table class="table table-hover">
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Full name</th>
      <th>Role</th>
      <th>Verified</th>
    </tr>

    <% @users.each do |user| %>
        <%= render 'user', user: user %>
    <%end %>


  </table>
</div>
